      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/(?!react-native).+\\.js$",
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/.history"
    ],
    "cacheDirectory": ".jest/cache",
    "automock": false,
    "setupFiles": [
      "./mockJest.js"
    ]
